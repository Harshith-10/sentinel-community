{
  "name": "java",
  "displayName": "Java",
  "extension": ".java",
  "command": "java",
  "args": [
    "-XX:+UseSerialGC",
    "-XX:TieredStopAtLevel=1",
    "-XX:CICompilerCount=1",
    "-Djava.security.egd=file:/dev/./urandom",
    "-Xms16m",
    "-Xmx128m",
    "-cp",
    "{dir}",
    "Main"
  ],
  "timeout": 30000,
  "compile": {
    "command": "javac",
    "args": [
      "-d",
      "{dir}",
      "{file}"
    ],
    "timeout": 10000
  },
  "filename": "Main.java"
}