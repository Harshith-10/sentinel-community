[
  {
    "name": "cpp",
    "displayName": "C++",
    "extension": ".cpp",
    "command": "./program",
    "args": [],
    "timeout": 30000,
    "compile": {
      "command": "g++",
      "args": [
        "-O2",
        "-pipe",
        "-s",
        "{file}",
        "-o",
        "program"
      ],
      "timeout": 10000
    },
    "dockerImage": "alpine:3.22",
    "dockerfileSetupCommands": [
      "apk add --no-cache g++ make"
    ],
    "k8s": {
      "replicas": 1,
      "cpu": "500m",
      "memory": "256Mi"
    }
  },
  {
    "name": "java",
    "displayName": "Java",
    "extension": ".java",
    "command": "java",
    "args": [
      "-XX:+UseSerialGC",
      "-XX:TieredStopAtLevel=1",
      "-XX:CICompilerCount=1",
      "-Djava.security.egd=file:/dev/./urandom",
      "-Xms16m",
      "-Xmx64m",
      "-cp",
      "{dir}",
      "Main"
    ],
    "timeout": 30000,
    "compile": {
      "command": "javac",
      "args": [
        "-d",
        "{dir}",
        "{file}"
      ],
      "timeout": 10000
    },
    "filename": "Main.java",
    "dockerImage": "eclipse-temurin:21-alpine",
    "k8s": {
      "replicas": 4,
      "cpu": "500m",
      "memory": "256Mi"
    }
  },
  {
    "name": "javascript",
    "displayName": "JavaScript",
    "extension": ".js",
    "command": "node",
    "args": [
      "{file}"
    ],
    "timeout": 30000,
    "dockerImage": "node:22-alpine",
    "k8s": {
      "replicas": 1,
      "cpu": "500m",
      "memory": "256Mi"
    }
  },
  {
    "name": "python",
    "displayName": "Python",
    "extension": ".py",
    "command": "python3",
    "args": [
      "{file}"
    ],
    "timeout": 30000,
    "dockerImage": "python:3.11-alpine",
    "k8s": {
      "replicas": 2,
      "cpu": "500m",
      "memory": "256Mi"
    }
  },
  {
    "name": "typescript",
    "displayName": "TypeScript",
    "extension": ".ts",
    "command": "node",
    "args": [
      "dist/main.js"
    ],
    "timeout": 30000,
    "dockerImage": "node:22-alpine",
    "k8s": {
      "replicas": 1,
      "cpu": "500m",
      "memory": "256Mi"
    },
    "dockerfileSetupCommands": [
      "npm install -g typescript"
    ],
    "compile": {
      "timeout": 10000,
      "command": "tsc",
      "args": [
        "{file}",
        "--outDir",
        "dist",
        "--target",
        "es2020",
        "--module",
        "commonJs",
        "--esModuleInterop"
      ]
    }
  }
]